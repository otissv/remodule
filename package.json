{
  "name": "remodule",
  "version": "0.0.3",
  "description": "Taming Redux with modules",
  "main": "dist/remodule",
  "keywords": ["redux", "react", "modules"],
  "scripts": {
    "build": "node ./node_modules/@graphql-guru/babel-tasks/tasks.js",
    "watch": "npm run build -- -w",
    "lint": "eslint --quiet './src' || true",
