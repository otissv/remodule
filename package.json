{
  "name": "remodule",
  "version": "1.0.0",
  "description": "Taming Redux with modules",
  "main": "dist/remodule",
  "keywords": ["redux", "react", "modules"],
  "scripts": {
    "build": "node ./node_modules/@graphql-guru/babel-tasks --src ./src/lib",
    "watch": "npm run build -- -w",
    "lint": "eslint --quiet './src' || true",
    "test": "ava --fail-fast server/__tests__",
    "test:spec": "ava server/__tests__ -v",
    "test:watch": "watch 'npm run test' ./server",
    "example": "react-scripts start"
  },
  "dependencies": {
    "@graphql-guru/babel-tasks": "^1.0.2",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-graphql": "^1.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "redux": "^3.7.1",
    "to-camel-case": "^1.0.0",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.13",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-thunk": "^2.2.0"
  }
}
